{
  "name": "vintage/yii2-search",
  "description": "Search in Active Record models for Yii2",
  "keywords": ["yii2", "component", "search", "active record"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/Vintage-web-production/yii2-search",
    "issues": "https://github.com/Vintage-web-production/yii2-search/issues"
  },
  "authors": [
    {
      "name": "Vladimir Kuprienko",
      "email": "vldmr.kuprienko@gmail.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13"
  },
  "require-dev": {
    "codeception/codeception": "~2.3",
    "codeception/verify": "*",
    "codeception/specify": "*"
  },
  "autoload": {
    "psr-4": {
      "vintage\\search\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "vintage\\search\\tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "scripts": {
    "test": [
      "codecept build",
      "codecept run"
    ]
  }
}
